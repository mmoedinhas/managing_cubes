*.ico filter=lfs diff=lfs merge=lfs -text lockable
*.gif filter=lfs diff=lfs merge=lfs -text lockable
*.hdf filter=lfs diff=lfs merge=lfs -text lockable
*.pgm filter=lfs diff=lfs merge=lfs -text lockable
*.tif filter=lfs diff=lfs merge=lfs -text lockable
# Compiled data
*.dll filter=lfs diff=lfs merge=lfs -text lockable
*.exe filter=lfs diff=lfs merge=lfs -text lockable
*.lib filter=lfs diff=lfs merge=lfs -text lockable
*.pdb filter=lfs diff=lfs merge=lfs -text lockable
